#
#  OSGi TEST BUNDLE 
#

Bundle-Version:1.0.0.${tstamp}
Bundle-Name: DOS-03 : DoS - CPU and RAM exhaustion - Scenario 3 - Version 1
Bundle-Description: Allocate large string arrays in Activator class attributes.

Private-Package: org.osgi.security.dos032.test
Test-Cases: ${test-cases}

-includeresource: {readme.md}

-buildpath: \
	osgi.enroute.base.api;version=1.0, \
	osgi.enroute.junit.wrapper;version=4.12

-runrequires: \
	osgi.identity;filter:='(osgi.identity=org.osgi.security.dos032.test)'

-runbundles: \
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.osgi.security.dos032.test;version=snapshot,\
	osgi.enroute.hamcrest.wrapper;version='[1.3.0,1.3.1)',\
	osgi.enroute.junit.wrapper;version='[4.12.0,4.12.1)'
-runproperties: org.osgi.framework.security=osgi