	#
#  OSGi TEST BUNDLE 
#

-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	osgi.enroute.junit.wrapper;version=4.12,\
	org.osgi.security.test.api;version=latest

Bundle-Version:1.0.0.${tstamp}
Bundle-Name: FING-06: Bundles extraction
Bundle-Description: Access (read) all bundles installed on the platform (with BundleContext.bundles or BundleContext.getBundles)


Private-Package: org.osgi.security.fing06.test
Test-Cases: ${test-cases}

-includeresource: {readme.md}


-runrequires: osgi.identity;filter:='(osgi.identity=org.osgi.security.fing06.test)'

-runbundles: \
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.osgi.security.fing06.test;version=snapshot,\
	org.osgi.security.test.api;version=snapshot,\
	osgi.enroute.hamcrest.wrapper;version='[1.3.0,1.3.1)',\
	osgi.enroute.junit.wrapper;version='[4.12.0,4.12.1)'